{
  "name": "podcasts-search",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "html-react-parser": "^3.0.4",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.1",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "xml-js": "^1.6.11"
  },
  "scripts": {
    "start": "node index",
    "start-dev": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test:coverage": "react-app-rewired test --coverage -u --watchAll=false",
    "test": "react-app-rewired test",
    "test:update-mocks": "react-app-rewired test -u",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "assert": "^2.0.0",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "https-browserify": "^1.0.0",
    "jest": "^29.1.2",
    "jest-fetch-mock": "^3.0.3",
    "os-browserify": "^0.3.0",
    "process": "^0.11.10",
    "react-app-rewired": "^2.2.1",
    "sass": "^1.55.0",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "url": "^0.11.0"
  },
  "jest": {
    "automock": false,
    "resetMocks": false,
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "coverageReporters": [
      "json",
      "html"
    ],
    "collectCoverageFrom": [
      "./src/components/*.js",
      "./src/pages/*.js",
      "./src/utils/*.js",
      "./!src/utils/context/*.js",
      "./!src/utils/context/*.js"
    ]
  }
}
